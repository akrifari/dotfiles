! Global config
URxvt.font: xft:Meslo LG S DZ for Powerline:pixelsize=15
URxvt.scrollBar: false
URxvt.iso14755: false
URxvt.iso14755_52: false

! Extensions
URxvt.perl-ext-common: default,resize-font,tabbedex

! Tabbedex settings
URxvt.tabbed.autohide: yes
URxvt.keysym.C-n: perl:tabbedex:new_tab
URxvt.keysym.C-h: perl:tabbedex:prev_tab
URxvt.keysym.C-l: perl:tabbedex:next_tab
