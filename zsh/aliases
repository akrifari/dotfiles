alias cat="bat"
alias f="nnn"
alias ga="git add"
alias gcm="git commit"
alias glo="git log --oneline --graph"
alias grf="git checkout HEAD --"
alias gst="git status"
alias npml="npm list -g --depth=0"
alias phps="php -S localhost:8000"
alias ptp="sudo powertop --auto-tune"
alias pts="ptp && ts"
alias pyhs="python -m http.server"
alias rm="rm -i"
alias ta="tmux attach -t"
alias tls="tmux ls"
alias tree="tree -I '*.git'"
alias ts="sudo tlp start"
alias tsh="tsp -h"
alias tsv="tsp -v"
alias vim="nvim"

